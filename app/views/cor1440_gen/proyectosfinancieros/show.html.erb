<%- model_class = Cor1440Gen::Proyectofinanciero -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= Cor1440Gen::Proyectofinanciero.human_attribute_name(:id) %>:</strong></dt>
  <dd><%= @proyectofinanciero.id %></dd>
  <dt><strong><%= Cor1440Gen::Proyectofinanciero.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @proyectofinanciero.nombre %></dd>
  <dt><strong><%= Cor1440Gen::Proyectofinanciero.human_attribute_name(:compromisos) %>:</strong></dt>
  <dd><%= @proyectofinanciero.compromisos %></dd>
  <dt><strong><%= Cor1440Gen::Proyectofinanciero.human_attribute_name(:fechacierre) %>:</strong></dt>
  <dd><%= @proyectofinanciero.fechacierre %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
    proyectosfinancieros_ruta, :class => 'btn'  
  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
    edita_proyectofinanciero_ruta(@proyectofinanciero), :class => 'btn' 
  %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
    proyectofinanciero_ruta(@proyectofinanciero),
    :method => 'delete',
    :data => { :confirm => 
               t('.confirm', :default => t("helpers.links.confirm",
                                           :default => '¿Está seguro(a)?')) },
    :class => 'btn btn-danger' 
  %>
</div>
