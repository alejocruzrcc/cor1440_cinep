<%- model_class = Cor1440Gen::Proyectofinanciero -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-offset-0 col-md-1"> 
      <% if @proyectosfinancieros.size > 0 %>
        <%= link_to t('.new', :default => t("helpers.links.new")),
          nuevo_proyectofinanciero_ruta,
          :class => 'btn btn-primary' 
        %>
      <% end %>
    </div>
    <div class="col-md-offset-3 col-md-5"><%= model_class.model_name.human.pluralize.titleize %>: <span id="numproyectosfinancieros"><%= @numproyectosfinancieros %></span>
    </div>
  </div>
</div>

<div id="casos">
  <%= render 'index_tabla' %>
</div>

<%= link_to t('.new', :default => t("helpers.links.new")),
  nuevo_proyectofinanciero_ruta,
  :class => 'btn btn-primary' 
%>


