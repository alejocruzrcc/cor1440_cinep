<% col = ::Actor.where(
  'id IN (SELECT DISTINCT actor_id ' + 
  '  FROM actividad_actor)') %>
<%= f.input :busactor,
  collection: col.order(:nombre),
  include_blank: true,
  label_method: :nombre,
  value_method: :id,
  label: false,
  selected: Sip::ModeloHelper.poromision(params, :busactor),
  input_html: {
    class: 'chosen-select',
    'data-enviarautomatico' => ''
  }
%>
