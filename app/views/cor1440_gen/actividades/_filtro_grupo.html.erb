<% col = Sip::Grupo.where(
  'id IN (SELECT DISTINCT grupo_id FROM actividad_grupo)') %>
<%= f.input :busgrupo,
  collection: col.habilitados,
  include_blank: true,
  label_method: :presenta_nombre,
  value_method: :id,
  label: false,
  selected: Sip::ModeloHelper.poromision(params, :busgrupo),
  input_html: {
    class: 'chosen-select',
    'data-enviarautomatico' => ''
  }
%>
