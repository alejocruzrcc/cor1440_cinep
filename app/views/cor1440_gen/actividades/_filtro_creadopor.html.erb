<% col = ::Usuario.where(
  'id IN (SELECT DISTINCT creadopor_id ' + 
  '  FROM cor1440_gen_actividad)') %>
<%= f.input :buscreadopor,
  collection: col.order(:nusuario),
  include_blank: true,
  label_method: :nusuario,
  value_method: :id,
  label: false,
  selected: Sip::ModeloHelper.poromision(params, :buscreadopor),
  input_html: {
    class: 'chosen-select',
    'data-enviarautomatico' => ''
  }
%>
