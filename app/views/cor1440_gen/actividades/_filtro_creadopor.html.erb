<% col = ::Usuario.where(
  'id IN (SELECT DISTINCT id ' + 
  '  FROM usuario)') %>
<%= f.input :buscreadopor,
  collection: col.order(:nombres, :apellidos),
  include_blank: true,
  label_method: :presenta_nombre,
  value_method: :id,
  label: false,
  selected: Sip::ModeloHelper.poromision(params, :buscreadopor),
  input_html: {
    class: 'chosen-select',
    'data-enviarautomatico' => ''
  }
%>
