<tr class='nested-fields'>
  <td>
    <%= f.input :fechaini_localizada, :input_html => { 
      "data-behaviour" => "datepicker" },
      label: false,
      :as => :string %>
  </td>
  <td>
    <%= f.input :fechafin_localizada, :input_html => { 
      "data-behaviour" => "datepicker" },
      label: false,
      :as => :string %>
  </td>
  <td>
    <%= f.input :observaciones,
      label: false %>
  </td>
  <td>
    <%= f.input :id, as: :hidden %>
    <%= link_to_remove_association "Eliminar", f, 
        :class => 'btn-danger' 
    %>
    <div><%= f.error_notification %></div>
  </td>
</tr>


